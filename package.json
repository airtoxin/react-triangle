{
  "name": "react-triangle",
  "version": "0.3.0",
  "description": "draw svg triangle grid with react",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch",
    "dev": "run-p watch storybook",
    "test": "run-p test:*",
    "test:lint": "eslint {src,test,stories} --fix",
    "test:unit": "mocha --require jsdom-global/register --compilers jsx:babel-register --recursive test",
    "test:storybook": "NODE_ENV=test storyshots",
    "prepublish": "run-s test build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airtoxin/react-triangle.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "svg",
    "polygon",
    "triangle",
    "grid"
  ],
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airtoxin/react-triangle/issues"
  },
  "homepage": "https://github.com/airtoxin/react-triangle#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.35.3",
    "@kadira/storyshots": "2.1.0",
    "@personare/react-storybook-decorator-github-corner": "0.1.5",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babelify": "7.3.0",
    "browserify": "13.3.0",
    "enzyme": "2.7.0",
    "eslint": "3.13.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "is": "3.2.0",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "seedrandom": "2.4.2"
  },
  "dependencies": {
    "babel-runtime": "6.20.0"
  }
}
